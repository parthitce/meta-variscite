# LAYER_CONF_VERSION is increased each time build/conf/bblayers.conf
# changes incompatibly
LCONF_VERSION = "7"

BBPATH = "${TOPDIR}"
BBFILES ?= ""

YOCTO_LAYERS = " \
  ##OEROOT##/meta \
  ##OEROOT##/meta-poky \
  ##OEROOT##/meta-yocto-bsp \
  "

EXTRA_LAYERS = " \
  ##OEROOT##/meta-variscite \
  "

BBLAYERS ?= "${YOCTO_LAYERS} ${EXTRA_LAYERS}"
